@model BestellserviceWeb.Models.TblProjekt

@{
    ViewData["Title"] = "Details";
    List<TblLeistungsbilderProjekt> leistungsbilder = ViewBag.leistungsbilder;
}

<h1>Details</h1>

<div>
    <h4>TblProjekt</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ProjName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ProjName)
        </dd>
    </dl>
</div>

<table class="table">
    <tbody>
@foreach (var item in leistungsbilder) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.LeistpId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LeistpLeistungsbildNavigation.LeistName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LeistpAmount)
            </td>
        </tr>
}
    </tbody>
</table>
<div>
    <a asp-action="Edit" asp-route-id="@Model.ProjId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
